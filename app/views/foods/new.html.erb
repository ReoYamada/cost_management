<h1>商品登録</h1>
<%= form_for(@food, url: foods_path, method: :post) do |f| %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <div>
    食材名<%= f.text_field :name %>
  </div>
  <div>
    キログラム単価<%= f.number_field :unit_price_per_kilo %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
